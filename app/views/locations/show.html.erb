<%= render 'locations/location', location: @location %>

<div class="text-center edit-links">
	<%= link_to "Back", locations_path %>
	|
	<%= link_to "Edit", edit_location_path(@location) %>
	|
	<%= link_to "Delete", location_path(@location), method: :delete, data: { confirm: "Are you sure you want to delete this location?" } %>
</div>